{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        // "plugin:@typescript-eslint/recommended"
        "plugin:node/recommended"

    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "prettier"
        // "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off"

    }
}
